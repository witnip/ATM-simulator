<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.witnip.atm.ATMController">
<children>
    <Label layoutX="154.0" layoutY="29.0" text="ATM simulator">
        <font>
            <Font name="Verdana Bold" size="36.0" />
        </font>
    </Label>
    <VBox layoutX="196.0" layoutY="100.0" prefHeight="200.0" prefWidth="209.0">
        <children>
            <TextField fx:id="txtAmount" promptText="Enter Amont">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </VBox.margin>
            </TextField>
            <TextField fx:id="txtAccountNumber" promptText="Account Number">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </VBox.margin>
            </TextField>
            <Button fx:id="btnTransferMoney" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Transfer Money">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </VBox.margin>
            </Button>
            <Button fx:id="btnCheckBalance" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Check Balance">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </VBox.margin>
            </Button>
            <Button fx:id="btnChangePassword" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Change Password">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </VBox.margin>
            </Button>
        </children>
    </VBox>
</children>
</AnchorPane>
